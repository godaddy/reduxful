{
  "name": "reduxful",
  "version": "1.0.2",
  "description": "Redux state from RESTful services",
  "main": "./lib",
  "browser": "./lib",
  "module": "./src",
  "license": "MIT",
  "author": "GoDaddy Operating Company, LLC",
  "contributors": [
    "Andrew Gerard <agerard@godaddy.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/godaddy/reduxful.git"
  },
  "keywords": [
    "redux",
    "state",
    "rest",
    "api",
    "action",
    "reducer",
    "selector",
    "flux"
  ],
  "scripts": {
    "build": "babel src -d lib --ignore '**/*.spec.js'",
    "lint": "eslint src/ tests/ *.js",
    "lint:fix": "yarn lint --fix",
    "prepublish": "yarn build",
    "pretest": "yarn lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "docs:react-addons": "jsdoc2md src/addons/react-addons.js > docs/react-addons-api.md",
    "docs:api": "jsdoc2md src/*.js > docs/api.md",
    "docs": "yarn docs:api && yarn docs:react-addons"
  },
  "dependencies": {
    "es6-error": "^4.1.1",
    "prop-types": "^15.5.10",
    "transform-url": "^1.0.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-jest": "^23.4.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "coveralls": "^3.0.2",
    "eslint": "^5.5.0",
    "eslint-config-godaddy": "^2.1.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-mocha": "^5.0.0",
    "jest": "^23.5.0",
    "jsdoc-to-markdown": "^4.0.1"
  }
}
